<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <Button
        android:id="@+id/Button_string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取得字符串数据" />

    <Button
        android:id="@+id/Button_json"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取得JSON数据" />

    <Button
        android:id="@+id/Button_gjson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取得GJSON数据" />

    <Button
        android:id="@+id/Button_xml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取得XML数据" />

    <Button
        android:id="@+id/Button_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取得图片" />

    <Button
        android:id="@+id/Button_cacheImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取得缓存图片" />
</LinearLayout>

